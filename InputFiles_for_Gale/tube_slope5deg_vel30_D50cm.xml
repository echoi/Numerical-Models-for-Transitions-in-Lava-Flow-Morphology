<!-- ============== Conversion Factors  ================  -->
<!-- T = 10^2 (sec) * T* -->
<!-- M = 10^4 (kg) * M* -->
<!-- eta_ref=100 Pa.s -->
<!-- v_ref=0.1 m/s -->
<!-- kappa_ref=1.0e-6 m^2/s -->
<!-- eta_ref* = [L][T]/[M]*eta_ref = (1*10^2)/10^4*100 = 1.0 -->
<!-- v* = [T]/[L]*v = 10^2/1*0.1 = 10.0 -->
<!-- kappa* = [T]/[L^2]*kappa_ref = 10^2/1*1.0e-6 = 1.0e-4 -->
